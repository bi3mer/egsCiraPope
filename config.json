{
	"dev": false,
	"filter": "#USA",
	"mongo": {
		"url": "mongodb://localhost/test",
		"modelTweet": "tweets",
		"modelStats": "countryStats",
		"options": {
			"db": { 
				"native_parser": true 
			}, 
			"server": { 
				"poolSize": 5,
				"keepAlive": 1 
			}
		}
	},
	"auth": {
		"directory": "./authenticationLayer/dirtyWords/",
		"countryCodeLength": 4,
		"lists": ["zh", "cs", "da", "nl", "en", "eo", "fi", "fr", "de", "hu", "it", "ja", "tlh", "ko", "no", "fa", "pl", "pt", "ru", "es", "sv", "th", "tr"]
	},
	"server": {
		"port": 3000,
		"paths": {
			"getMap": "/getMapData",
			"getTweet": "/getTweet"
		}, 
		"tweets": {
			"noTweetFoundText": "Check back later after tweeting has begun"
		}
	},
	"webApp": {
		"requestMapTimeOut": 1000,
		"tweetTimeout": 10000,
		"mapTimeOut": 15000,
		"divStyle": {
			"position": "relative",
			"width": "1000px",
			"height": "600px",
			"margin-left": "auto",
    		"margin-right": "auto"
		},
		"colors": {
			"very hot": "#5c1d1f",
			"hot": "#963032",
			"Light hot": "#c64d4f",
			"Light cold": "#4d8cc6",
			"cold": "#3672a9",
			"very cold": "#2a5883",
			"defaultFill": "#1d3e5c"
		},
		"colorPercentages": [
			{
				"color": "very hot",
				"percentage": 0.9
			}, 
			{
				"color": "hot",
				"percentage": 0.8
			}, 
			{
				"color": "Light hot",
				"percentage": 0.6
			}, 
			{
				"color": "Light cold",
				"percentage": 0.4
			}, 
			{
				"color": "cold",
				"percentage": 0.3
			}, 
			{
				"color": "very colod",
				"percentage": 0.1
			}]
	}
}
